<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="io.wwphsstechdesk.application.Main">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name="io.wwphsstechdesk.application.Main.MainActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="io.wwphsstechdesk.application.Main.FAQActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity
            android:name="io.wwphsstechdesk.application.Main.ScrollableActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity
            android:name="io.wwphsstechdesk.application.Main.WifiActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity
            android:name="io.wwphsstechdesk.application.Main.LaptopChromebookActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity
            android:name="io.wwphsstechdesk.application.Main.ProjectorActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
    </application>

</manifest>